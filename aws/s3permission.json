{
	"Version": "2012-10-17",
	"Id": "PolicyForDestinationBucket",
	"Statement": [
		{
			"Sid": "Permissions on objects and buckets",
			"Effect": "Allow",
			"Principal": {
				"AWS": "arn:aws:iam::199183545087:user/thanapon"
			},
			"Action": "s3:*",
			"Resource": [
				"arn:aws:s3:::mmmultipro301",
				"arn:aws:s3:::mmmultipro301/*"
			],
			"Action": [
				"s3:ListBucket",
				"s3:GetObject",
				"s3:PutObject",
				"s3:PutObjectAcl"
			],
			"Resource": [
				"arn:aws:s3:::mmmkultipro301",
				"arn:aws:s3:::mmkmultipro301/*"
			]
		}
	]
},

